{
  "manifest_version": 3,
  "name": "AI绘画提示词管理器",
  "version": "1.0.0",
  "description": "管理和使用AI绘画提示词，支持飞书表格同步",
  "permissions": ["storage", "alarms", "sidePanel"],
  "host_permissions": ["https://open.feishu.cn/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "options_page": "options.html",
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
